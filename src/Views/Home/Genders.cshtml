@{
    ViewData["Title"] = "Genders";
}
<h2>@ViewData["Title"]</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    @foreach (var gender in ViewData["allGenders"] as List<Gender>) {
      <tr>
        <td>@gender.Name</td>
      </tr>
      }
  </tbody>
</table>
